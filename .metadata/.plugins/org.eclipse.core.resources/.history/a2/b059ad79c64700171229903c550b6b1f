package workspace;

public class SuperTrunfo {

	public static void main(String[] args) {
		Baralho baralho = new Baralho();
		int size = baralho.size();
		
		for (int i = 0; i < size; i++) {
			System.out.printf("%c%c ", baralho.get(i).getNum(), baralho.get(i).getId());
		} System.out.printf("\n");
		
		baralho.shuffle();
		for (int i = 0; i < size; i++) {
			System.out.printf("%c%c ", baralho.get(i).getNum(), baralho.get(i).getId());
		} System.out.printf("\n");
	}

}
